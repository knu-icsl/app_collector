<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="icsl.apps.collector">
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pdr">
        <activity
            android:name="icsl.apps.collector.SensorActivity"
            android:label="Sensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="icsl.apps.collector.MainActivity"/>
        </activity>
        <activity
            android:name="icsl.apps.collector.WifiActivity"
            android:label="WiFi">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="icsl.apps.collector.MainActivity" />
        </activity>
        <activity
            android:name="icsl.apps.collector.SettingsActivity"
            android:label="Settings">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="icsl.apps.collector.MainActivity" />
        </activity>
        <activity android:name="icsl.apps.collector.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>